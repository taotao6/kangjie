
page {
	background-color: #f6f6f6;
	box-sizing: border-box;
}
.box-wrap{
	background-color: #fff;
	border-radius: 20rpx;
}
.screen-swiper {
	height: 600rpx;
	position: relative;
}
.back{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	top: 20rpx;
	right: 30rpx;
}
.close-Icon{
	width: 33rpx;
	height: 33rpx;
	margin-left: 250rpx;
}
.desc {
	width: 100%;
	padding: 20rpx 0;
	background-color: #fff;
}
.desc-in {
	width: 92%;
	margin: 0 auto;
}
.desc-in-title {
	font-size: 32rpx;
	color: #333;
	line-height: 1.8;
}
.title-icon {
	color: #0daafb;
	margin-right: 20rpx;
}
.desc-in-tip {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	padding: 10rpx;
	background-color: rgb(238, 252, 254);
	color: #009cec;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 20rpx 0;
}
.tip-left {
	font-size: 24rpx;
}
.tip-right {
	font-size: 20rpx;
}
.desc-in-price {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	color: #666;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-bottom: 10rpx;
	-webkit-box-align: baseline;
	-webkit-align-items: baseline;
	        align-items: baseline;
}
.price-before {
	font-size: 42rpx;
	color: #ee0000;
}
.price-after {
	font-size: 32rpx;
	color: #ee0000;
}
.price-right {
	font-size: 20rpx;
	color: #999;
}
.desc-in-time {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	padding: 30rpx 0;
	-webkit-box-align: baseline;
	-webkit-align-items: baseline;
	        align-items: baseline;
	margin: 30rpx 0;
	background-color: #fff;
}
.desc-time-in {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 92%;
	margin: 0 auto;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.time-before {
	font-size: 26rpx;
	color: #333;
}
.time-after {
	margin-left: 20rpx;
	font-size: 24rpx;
	color: #666;
}
.time-right {
	width: 12rpx;
	height: 22rpx;
}
.good-detail {
	width: 100%;
	padding-bottom: 100rpx;
}
.detail-title {
	text-align: center;
	font-size: 26rpx;
	margin-bottom: 30rpx;
}
.detail-img {
	width: 100%;
	height: 500rpx;
	background: url(http://taotao111.test.upcdn.net/1.jpg) no-repeat center center;
	background-size: cover;
}
.detail-img-1 {
	width: 100%;
	height: 500rpx;
	background: url(http://taotao111.test.upcdn.net/2.jpg) no-repeat center center;
	background-size: cover;
}
.detail-img-2 {
	width: 100%;
	height: 500rpx;
	background: url(http://taotao111.test.upcdn.net/3.jpg) no-repeat center center;
	background-size: cover;
}
.detail-img-3 {
	width: 100%;
	height: 500rpx;
	background: url(http://taotao111.test.upcdn.net/4.jpg) no-repeat center center;
	background-size: cover;
}
.nav-people-img,
.nav-cart-img {
	width: 37rpx;
	height: 39rpx;
}
.nav-cart{
	position: relative;
}
.nav-cart-icon{
	position: absolute;
	top: 0rpx;
	left: 100rpx;
	width: 24rpx;
	height: 24rpx;
	border-radius: 100%;
	background-color: red;
	font-size: 18rpx;
	color: #fff;
	text-align: center;
	line-height: 24rpx;
	/* padding: 6upx; */
}
.bottom-nav {
	width: 100%;
	height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	position: fixed;
	bottom: 0;
	background-color: #fff;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.nav-people {
	width: 25%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 22rpx;
}
.nav-cart {
	width: 25%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 22rpx;
}
.nav-add {
	width: 25%;
	height: 100%;
	line-height: 100rpx;
	text-align: center;
	background-color: #85e1ff;
	color: #fff;
	font-size: 28rpx;
}
.nav-buy {
	width: 25%;
	height: 100%;
	line-height: 100rpx;
	text-align: center;
	background-color: #0daafb;
	color: #fff;
	font-size: 28rpx;
}
.demo-popup {
	background-color: #fff;
	/* height: 400upx; */
	border-radius: 10rpx 10rpx 0 0;
}
.good-top {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 20rpx;
	border-bottom: 1px solid #eee;
}
.good-top-left {
	width: 230rpx;
	height: 210rpx;
}
.good-top-left image {
	object-fit: cover;
}
.good-top-right {
	margin-left: 20rpx;
}
.good-top-right .price {
	margin-top: 20rpx;
	color: #f10000;
	font-size: 40rpx;
	margin-bottom: 20rpx;
}
.good-top-right .number {
	color: #999;
	font-size: 24rpx;
	margin-bottom: 20rpx;
}
.good-top-right .time {
	color: #999;
	font-size: 24rpx;
}
.good-center {
	padding: 30rpx 20rpx 20rpx 20rpx;
	border-bottom: 1px solid #eee;
}
.good-center-top {
	font-size: 28rpx;
	color: #333;
}
.good-center-footer {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 20rpx;
	padding-bottom: 30rpx;
}
.footer-1 {
	border: none;
	border-radius: 40rpx;
	padding: 10rpx 50rpx;
	margin-left: 30rpx;
	background-color: #f5f5f5;
}
.footer-1.active {
	border: 1px solid #007aff;
	color: #007aff;
	border-radius: 40rpx;
	background-color: #fff;
}
.good-footer {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 30rpx 20rpx;
	font-size: 28rpx;
}
.good-footer-right .number {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	font-size: 40rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.number .shu {
	width: 50rpx;
	margin-left: 30rpx;
	background-color: #ebeef5;
	text-align: center;
	margin-right: 30rpx;
}
.comfirm {
	width: 100%;
	height: 97rpx;
	background-color: #007aff;
	color: #fff;
	font-size: 32rpx;
	line-height: 97rpx;
	text-align: center;
}
.moreTip {
	width: 470rpx;
	height: 649rpx;
	background-color: #fff;
	padding: 20rpx;
	border-radius: 10rpx;
}
.tipTitle {
	font-size: 32rpx;
	font-weight: 600;
}
.tipList-detail {
	font-size: 24rpx;
	margin-top: 20rpx;
}
.haveRead {
	width: 430rpx;
	height: 70rpx;
	text-align: center;
	line-height: 70rpx;
	color: #0daafb;
	font-size: 32rpx;
	border: 1px solid #0daafb;
	margin-top: 30rpx;
}

