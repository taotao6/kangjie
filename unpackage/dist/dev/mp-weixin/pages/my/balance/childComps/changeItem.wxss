
.change-item{
	background-color: #fff;
	width: 100%;
}
.change-item-in{
	width: 92%;
	margin: 0 auto;
	padding: 20rpx 0;
	border-bottom: 1rpx solid #EEEEEE;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.item-left-top{
	font-size: 30rpx;
	color: #333;
}
.item-left-bottom{
	font-size: 24rpx;
	color: #B2B2B2;
	margin-top: 10rpx;
}
.item-right{
	font-size: 34rpx;
}

